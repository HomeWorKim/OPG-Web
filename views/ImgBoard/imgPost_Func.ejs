<%

getimgsrc = function getimgsrc(data) {
  var imgsrc;
  if(data.images[0] !== "") {
    return data.images[0];
  }
  else if (data.fileOriginalname && ((/\.(jpe?g|png|gif|bmp)$/i).test(data.fileOriginalname))) {
    imgsrc = data.filePath;
  }
  else {
    imgsrc = '/images/No_image_available.jpg';
  }
    return imgsrc;
  }



viewallimg = function viewallimage(data){
  if( (/\.(jpe?g|png|gif|bmp)$/i).test(data.filename) ) {
    return {
      filename : ('/uploadedfiles/'+ data.filename)
    };
  }
/*  else {
    return { default : '/images/No_image_available.jpg' };
  }*/
}

getfilepath = function getfilepath(data) {
  return ('/uploadedfiles/'+ data.filePath);
}

%>
