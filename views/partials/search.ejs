<!--if you wanna use this in div    div should have id:imaginary_container  becuase this is a part of bootstrap_search-->
<form action="/<%=path%>">
<select id="sel" name="searchType">
<option value="title" <%= search.searchType=="title"?select="selected":""%>>제목</option>
<option value="author" <%= search.searchType=="author"?select="selected":""%>>작성자</option>
</select> 
<div class="input-group">
<input type="text" class="form-control"  placeholder="Search" name="searchText">
  <span class="input-group-addon">
      <button type="submit">
          <span class="glyphicon glyphicon-search"></span>
      </button>  
  </span>
</div>
</form>